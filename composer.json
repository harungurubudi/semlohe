{
    "name": "Semlohe",
    "description": "SSO buat semlohe",
    "require": {
        "silex/silex": "~2.0",
        "hassankhan/config": "^0.10.0",
        "monolog/monolog": "^1.23",
        "vlucas/phpdotenv": "^2.4",
        "firebase/php-jwt": "^5.0",
        "defuse/php-encryption": "^2.1",
        "league/fractal": "^0.17.0",
        "ramsey/uuid": "^3.7",
        "moontoast/math": "^1.1",
        "illuminate/events": "^5.3",
        "illuminate/database": "^5.3",
        "jguyomard/silex-capsule-eloquent": "~2.0",
        "illuminate/pagination": "^5.3",
        "robmorgan/phinx": "^0.9.2",
        "twig/twig": "^2.4",
        "symfony/twig-bridge": "^4.0",
        "jasongrimes/paginator": "~1.0",
        "swiftmailer/swiftmailer": "^6.0",
        "gumlet/php-image-resize": "^1.9",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^1.0"
    },
    "prefer-stable": true,
    "preferred-install": "dist",
    "autoload": {
        "psr-4": {
            "App\\Semlohe\\": "src/"
        },
        "files": ["bootstrap/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
          "App\\Semlohe\\Tests\\": "tests/"
        }
    }
}
